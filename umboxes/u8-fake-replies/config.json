    "COMPROMISE": ["ALERT", "EMAIL"],
    "FAILED_AUTH": ["ALERT", "EMAIL"]
  },

  "emailConfig": {
    "email_server_address": "10.64.22.12",
    "email_server_port": "25",
    "email_source_address": "kalki@sei.cmu.edu",
